{
	"name": "dynamite-sdk",
	"version": "1.0.0",
	"description": "a ts sdk for dynamodb",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"engines": {
		"node": "^14.15.0 || ^16.6.2",
		"yarn": "^1.22.5"
	},
	"scripts": {
		"prepare": "husky install",
		"lint": "eslint --fix --ext .ts .",
		"clean": "npx rimraf ./yarn.lock && npx rimraf ./lib && npx rimraf ./**/node_modules",
		"build": "tsc",
		"test": "jest",
		"pack:light": "yarn build && npm pack --dry-run",
		"pack:release": "yarn build && npm pack",
		"release": "yarn build && npm publish"
	},
	"author": "Jude Giordano",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^27.0.3",
		"@types/mocha": "^9.0.0",
		"@types/node": "^14.14.14",
		"@types/uuid": "^8.3.3",
		"@typescript-eslint/parser": "^4.10.0",
		"eslint": "^7.16.0",
		"husky": "^7.0.0",
		"jest": "^27.3.1",
		"ts-jest": "^27.0.7",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.2"
	},
	"dependencies": {
		"aws-sdk": "^2.1046.0",
		"class-transformer": "^0.5.1",
		"uuid": "^8.3.2"
	}
}
